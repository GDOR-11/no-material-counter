{
    "manifest_version": 3,
    "name": "no material counter",
    "description": "removes the material counter from chess.com so you can practice for OTB tournaments",
    "version": "1.0",
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/chess-com.js"],
            "matches": [ "https://www.chess.com/*" ]
        }
    ]
}
